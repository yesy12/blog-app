<h2>Lista de Categorias: </h2>
<hr>
<a href="/admin/categorias/add">
  <button class="btn btn-success">
    Nova Categoria
  </button>
</a>

<h3 class="mt-2">Categorias Criadas</h3>
{{#each categorias}}
  <div class="card mt-3">
    <div class="card-body">
      <h4>Nome {{nome}}</h4>
      <small>Slug {{slug}}</small>
    </div>
  </div>
{{/each}}